<ehcache updateCheck="false" name="shiroCache">
	<diskStore path="java.io.tmpdir" />
    <defaultCache
            maxElementsInMemory="10000"
            eternal="false"
            timeToIdleSeconds="1000"
            timeToLiveSeconds="1000"
            overflowToDisk="true"
            diskPersistent="false"
            diskExpiryThreadIntervalSeconds="500"
            />
</ehcache>